    <h1  id="site" class="active-sites-title">active-sites</h1>
    <div  class="sites">
      {{#data}}
        <!-- card -->
      <div class="site-card">
        <div class="site-card-img">
          <img src="./Images/pexels-lukas-rodriguez-3559235.jpg" alt="" />
        </div>
        <h2> {{name}}</h2>
        <div class="site-card-content">
