              <div class="number">65%</div>
            </div>
          </div>
          <svg
            xmlns="http://www.w3.org/2000/svg"
            version="1.1"
            width="160px"
            height="160px"
          >
            <defs>
              <linearGradient id="GradientColor">
                <stop offset="0%" stop-color="#e91e63" />
                <stop offset="100%" stop-color="#673ab7" />
              </linearGradient>
            </defs>
            <circle cx="80" cy="80" r="70" stroke-linecap="round" />
          </svg>
        </div>
        <a href="">
          <a href="./page1.html">
          <button type="button" >Visit</button>
        </a>
        </a>
      </div>
      </div>
      <!-- card-end -->
      
      {{/data}}
      <div class="modal-overlay">
      <div class="modal-container">
        <form action="/addsite">
          <label for="site-id">Site ID: </label>
          <input type="number" id="site-id" name="id"> <br> <br>
          <label for="site-name">Site Name: </label>
          <input type="text" id="site-name" name="name"> <br> <br>
          <label for="site-address">Site Address: </label>
          <input type="text" id="site-address" name="address" > <br> <br>
          <label for="date">Date: </label>
          <input type="date" id="date" name="date"> <br> <br>  
          <!-- Estimated quantity -->
          <label for="">Estimated Quantity:</label> <br> <br>
          <label for="bricks">Bricks :</label>
          <input type="number" id="brick" name="brickno">
          <span>kg</span> <br><br>
          <label for="cement">Cement :</label>
          <input type="number" id="cement" name="cementno">
          <span>kg</span> <br><br>
          <label for="steel">Steel :</label>
          <input type="number" id="steel" name="steelno">
          <span>kg</span> <br><br>
          <label for="sand">Sand :</label>
          <input type="number" id="sand" name="sandno">
          <span>kg</span> <br><br>
          <label for="aggregate">Aggregate :</label>
          <input type="number" id="aggregate" name="aggregateno">
          <span>kg</span> <br><br>
          <button type="submit" action="/addsite" >Submit</button>
        </form>
        <button class="close-btn"><i class="fas fa-times"></i></button>

      </div>
    </div>
      
